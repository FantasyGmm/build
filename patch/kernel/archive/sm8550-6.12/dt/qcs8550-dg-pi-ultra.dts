	usb0-sbu-mux {
		compatible = "gpio-sbu-mux";

		enable-gpios = <&tlmm 140 GPIO_ACTIVE_LOW>;
		select-gpios = <&tlmm 141 GPIO_ACTIVE_HIGH>;

		pinctrl-names = "default";
		pinctrl-0 = <&usb0_sbu_default>;

		mode-switch;
		orientation-switch;

		port {
			usb0_sbu_mux: endpoint {
				remote-endpoint = <&pmic_glink_sbu>;
			};
		};
	};

